cond_3 = if_else(any(diagnoses_split %in% peripheral_vascular_codes), 1, 0),
cond_4 = if_else(any(diagnoses_split %in% cerebrovascular_disease_codes), 1, 0),
cond_5 = if_else(any(diagnoses_split %in% dementia_codes), 1, 0),
